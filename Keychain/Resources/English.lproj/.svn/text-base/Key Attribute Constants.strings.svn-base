/* Key Attribute Constants. */

/* The key is the bit mask value as defined in cssmtype.h in Apple's Security framework.  Last updated for 10.4.10.  Note that since the key attribute is a bit map, you must mask out other bits first. */
/* The value is the constant corresponding to the code. */

0x00000001 = "CSSM_KEYATTR_PERMANENT";
0x00000002 = "CSSM_KEYATTR_PRIVATE";
0x00000004 = "CSSM_KEYATTR_MODIFIABLE";
0x00000008 = "CSSM_KEYATTR_SENSITIVE";
0x00000010 = "CSSM_KEYATTR_ALWAYS_SENSITIVE";
0x00000020 = "CSSM_KEYATTR_EXTRACTABLE";
0x00000040 = "CSSM_KEYATTR_NEVER_EXTRACTABLE";

0x10000000 = "CSSM_KEYATTR_RETURN_DATA";
0x20000000 = "CSSM_KEYATTR_RETURN_REF";
0x40000000 = "CSSM_KEYATTR_RETURN_NONE";